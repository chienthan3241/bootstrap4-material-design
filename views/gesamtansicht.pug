extends layout

block content
  .main.main-raised
    #events.cd-section.section.section-gray
      .cards
        .container
          .title
            h2.text-warning Gesamtansichten
          hr
          .row
            .col-lg-4.col-md-6
              .card.bg-primary
                .card-body
                  h5.card-category.card-category-social
                    i.fab.fa-accusoft
                    |  Baugrundstück & 3D Modell
                  h4.card-title
                    | Die Baugrundstück & 3D Modell (siehe unten)
              .card.card-blog
                .card-header.card-header-image
                  a(href="#" data-toggle="modal" data-target="#gs1")
                    img.img(src="/images/gs1.jpg")
                    .colored-shadow(style='background-image: url("/images/gs1.jpg"); opacity: 1;')
                .card-body
                  h6.card-category.text-info Unser Baugrundstück
                  p.card-description
                    | Blick nach Süden
              .card.card-blog
                .card-header.card-header-image
                  a(href="#" data-toggle="modal" data-target="#gs2")
                    img.img(src="/images/gs2.jpg")
                    .colored-shadow(style='background-image: url("/images/gs2.jpg"); opacity: 1;')
